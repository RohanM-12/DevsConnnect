

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

 model user {
  id Int  @id  @default(autoincrement())
  registrationNo String @unique 
  name String 
  email String 
  password String
  created_At DateTime @default(now())

 }
